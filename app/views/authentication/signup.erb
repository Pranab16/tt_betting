<h2>Sign Up</h2>
<div class="content_wrapper">
    <%= form_for @user, :url => signup_path(@user)  do |f| %>
        <p>
            Username: <%= f.text_field :username %>
        </p>
        <p>
            Email: <%= f.text_field :email %>
        </p>
        <p>
            Password: <%= f.password_field :password %>
        </p>
        <p>
            <%= f.submit 'Sign Up' %>
        </p>
    <% end %>
</div>